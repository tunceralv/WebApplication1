@model AdminTable

<form asp-action="AdminRegister" method="post">
    <div class="form-group">
        <label asp-for="AdminEmail"></label>
        <input asp-for="AdminEmail" class="form-control" />
        <span asp-validation-for="AdminEmail" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Adminsifre"></label>
        <input asp-for="Adminsifre" class="form-control" />
        <span asp-validation-for="Adminsifre" class="text-danger"></span>
    </div>


    <button type="submit" class="btn btn-primary">Kaydet</button>
</form>
